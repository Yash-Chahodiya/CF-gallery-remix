{
  "name": "",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "move-static": "cp -a ./node_modules/@pdftron/pdfjs-express/public/. ./dist/public/pdfjsexpress",
    "build": "remix vite:build",
    "postinstall": "node node_modules/@pdftron/webviewer/bin/copy-webviewer-files.js public/webviewer",
    "build:demo": "esbuild app/worker/demo.worker.tsx  --bundle --format=esm --outfile=public/demo.worker.bundle.js ",
    "build:demoClient": "esbuild app/worker/demoClient.tsx --bundle --format=esm --outfile=public/demoClient.bundle.js",
    "build:all": "npm run build:demo && npm run build:demoClient",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/pro-duotone-svg-icons": "^6.6.0",
    "@fortawesome/pro-light-svg-icons": "^6.5.2",
    "@fortawesome/pro-regular-svg-icons": "^6.5.2",
    "@fortawesome/pro-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@pdftron/pdfjs-express": "^8.7.5",
    "@pdftron/pdfjs-express-viewer": "^8.7.5",
    "@pdftron/webviewer": "^10.10.1",
    "@remix-run/node": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "@remix-run/serve": "^2.10.2",
    "antd": "^5.19.1",
    "axios": "^1.7.2",
    "comlink": "^4.4.1",
    "crypto-js": "^4.2.0",
    "esbuild": "^0.17.6",
    "isbot": "^4.1.0",
    "lightgallery": "^2.7.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-filerobot-image-editor": "^4.8.1",
    "remix-utils": "^7.6.0",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.2",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
